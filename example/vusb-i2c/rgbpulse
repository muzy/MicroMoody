#!/bin/sh

delay=16

vusb-i2cset 17   0 ${delay} 255   0   0 255 255
vusb-i2cset 17   1 ${delay}   0   0   0 255 255
vusb-i2cset 17   2 ${delay} 255 255   0 255 255
vusb-i2cset 17   3 ${delay}   0   0   0 255 255
vusb-i2cset 17   4 ${delay}   0 255   0 255 255
vusb-i2cset 17   5 ${delay}   0   0   0 255 255
vusb-i2cset 17   6 ${delay}   0 255 255 255 255
vusb-i2cset 17   7 ${delay}   0   0   0 255 255
vusb-i2cset 17   8 ${delay}   0   0 255 255 255
vusb-i2cset 17   9 ${delay}   0   0   0 255 255
vusb-i2cset 17  10 ${delay} 255   0 255 255 255
vusb-i2cset 17  11 ${delay}   0   0   0 255 255
vusb-i2cset 17 120        0   0   0   0 255 255
