#!/bin/sh

delay=12

vusb-i2cset 17 64 ${delay} 255   0   0 255 255
vusb-i2cset 17 65 ${delay}   0   0   0 255 255
vusb-i2cset 17 66 ${delay} 255 255   0 255 255
vusb-i2cset 17 67 ${delay}   0   0   0 255 255
vusb-i2cset 17 68 ${delay}   0 255   0 255 255
vusb-i2cset 17 69 ${delay}   0   0   0 255 255
vusb-i2cset 17 70 ${delay}   0 255 255 255 255
vusb-i2cset 17 71 ${delay}   0   0   0 255 255
vusb-i2cset 17 72 ${delay}   0   0 255 255 255
vusb-i2cset 17 73 ${delay}   0   0   0 255 255
vusb-i2cset 17 74 ${delay} 255   0 255 255 255
vusb-i2cset 17 75 ${delay}   0   0   0 255 255
vusb-i2cset 17 63        0   0   0   0 255 255
